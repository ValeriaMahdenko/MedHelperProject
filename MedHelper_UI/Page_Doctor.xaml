<Page x:Class="MedHelper_UI.Page_Doctor"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MedHelper_UI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page_Doctor">

    <Grid>
        <StackPanel x:Name="StackPP" Orientation="Vertical" Width="50" VerticalAlignment="Stretch" HorizontalAlignment="Left" Background="#FF56C1CA">
            <ScrollViewer Height="270" Width="50" Name="leftBarScroll" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled" VerticalAlignment="Center" Margin="0,40,0,40" Background="#FFED635E" Foreground="White" BorderBrush="#FFED635E" HorizontalAlignment="Stretch" >
                <StackPanel x:Name="StackP" VerticalAlignment="Center" HorizontalAlignment="Stretch" Width="30" Height="Auto" >
                    
                </StackPanel>
            </ScrollViewer>
        </StackPanel>

        <Frame x:Name="DoctorFrame" Margin="50,0,0,0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Page>
